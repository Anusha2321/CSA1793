my_list=[1,2,3]
my_list.append(4)
print("append:", my_list)
my_list.extend([5,6])
print("extend:", my_list)
my_list.remove(3)
print("extend:", my_list)
my_list.insert(1,10)
print("insert:", my_list)

#output
append: [1, 2, 3, 4]
extend: [1, 2, 3, 4, 5, 6]
extend: [1, 2, 4, 5, 6]
insert: [1, 10, 2, 4, 5, 6]



